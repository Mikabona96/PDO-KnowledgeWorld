                            <div class="select-wrapper select-wrapper-booking-type">
                                <div class="select-box">
                                    <div class="options-container">
                                        <div class="option">
                                            <input type="radio" class="radio" id="" name="category" />
                                            <label data-value="0" for="news">Individual/Family</label>
                                        </div>

                                        <div class="option">
                                            <input type="radio" class="radio" id="" name="category" />
                                            <label data-value="1" for="magazine">Individual</label>
                                        </div>

                                        <div class="option">
                                            <input type="radio" class="radio" id="" name="category" />
                                            <label data-value="2" for="publication">Family</label>
                                        </div>
                                    </div>

                                    <div class="selected">
                                        Individual/Family
                                    </div>
                                </div>
                            </div>
                            <input type="text" placeholder="Your Name" class="name">
                            <input type="text" placeholder="Your second name" class="second-name">
                            <div class="code-number">
                                <div class="select-wrapper select-wrapper-code">
                                    <div class="select-box">
                                        <div class="options-container">
                                            <div class="option">
                                                <input type="radio" class="radio" id="" name="category" />
                                                <label data-value="+1" for="news">+1</label>
                                            </div>

                                            <div class="option">
                                                <input type="radio" class="radio" id="" name="category" />
                                                <label data-value="+2" for="magazine">+2</label>
                                            </div>

                                            <div class="option">
                                                <input type="radio" class="radio" id="" name="category" />
                                                <label data-value="+9" for="publication">+9</label>
                                            </div>
                                        </div>

                                        <div class="selected">
                                            +1
                                        </div>
                                    </div>
                                </div>
                                <input type="text" placeholder="Number" class="number">
                            </div>
                            <div class="checkbox-wrapper checkbox-wrapper-sms">
                                <label class="container"> Send Booking Reminders to me by SMS
                                    <input type="checkbox" checked="checked">
                                    <span class="checkmark"></span>
                                </label>
                            </div>
                            <input class="email" placeholder="Email adress" type="email" name="email" id="">
                            <div class="checkbox-wrapper checkbox-wrapper-newsletter">
                                <label class="container"> Sign up for our Knowledge World newsletter
                                    <input type="checkbox" checked="checked">
                                    <span class="checkmark"></span>
                                </label>
                            </div>
                            <div class="select-wrapper select-wrapper-adult-visitors">
                                <div class="select-box">
                                    <div class="options-container">
                                        <div class="option">
                                            <input type="radio" class="radio" id="" name="category" />
                                            <label data-value="1" for="news">1</label>
                                        </div>

                                        <div class="option">
                                            <input type="radio" class="radio" id="" name="category" />
                                            <label data-value="2" for="magazine">2</label>
                                        </div>

                                        <div class="option">
                                            <input type="radio" class="radio" id="" name="category" />
                                            <label data-value="2" for="publication">3</label>
                                        </div>
                                    </div>

