                                        <div class="option">
                                            <input type="radio" class="radio" id="" name="category" />
                                            <label data-value="09:00 - 10:00" for="magazine">09:00 - 10:00</label>
                                        </div>

                                        <div class="option">
                                            <input type="radio" class="radio" id="" name="category" />
                                            <label data-value="10:00 - 11:00" for="publication">10:00 - 11:00</label>
                                        </div>
                                    </div>

                                    <div class="selected">
                                        08:00 - 09:00
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <button class="btn next-step">NEXT STEP</button>
                <button class="btn prev-step">STEP BACK</button>
            </div>
